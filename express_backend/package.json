{
  "name": "express-posts",
  "version": "1.0.0",
  "engines": {
    "node": "10.16.0"
  },
  "description": "small REST API for posts",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "client": "npm run serve --prefix ./vue_frontend",
    "clean": "cd ./vue_frontend && rm -rf ./node_modules && cd .. && rm -rf ./node_modules",
    "installmods": "npm install && cd ./vue_frontend && npm install && cd ..",
    "both": "concurrently \"npm run dev\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix ./vue_frontend && npm run build --prefix ./vue_frontend"
  },
  "author": "LS",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.5",
    "multer": "^1.4.2",
    "uuid": "^3.3.3"
  }
}
